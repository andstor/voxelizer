{
  "name": "voxelizer",
  "version": "1.0.1",
  "description": "Voxelization for 3D models",
  "main": "lib/voxelizer.cjs",
  "module": "lib/voxelizer.js",
  "umd": "lib/voxelizer.umd.js",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "build-r": "rollup -c",
    "watch-r": "rollup -c -w",
    "test": "jest --passWithNoTests",
    "coverage": "npm test -- --coverage --coverageReporters=lcov",
    "prepare": "npm run build",
    "example": "browser-sync start --server --files 'examples' 'lib' --startPath ''examples/index.html"
  },
  "exports": {
    ".": {
      "import": "./lib/voxelizer.js",
      "require": "./lib/voxelizer.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andstor/voxelizer.git"
  },
  "keywords": [
    "voxelization",
    "voxel"
  ],
  "author": {
    "name": "Andr√© Storhaug",
    "email": "andr3.storhaug@gmail.com",
    "url": "https://github.com/andstor"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andstor/voxelizer/issues"
  },
  "files": [
    "/lib",
    "/src"
  ],
  "homepage": "https://github.com/andstor/voxelizer#readme",
  "devDependencies": {
    "@andrewleedham/rollup-plugin-auto-external": "^3.0.0",
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "canvas": "^2.6.1",
    "coveralls": "^3.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.13.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "three": "^0.x.x",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "binvox": "^1.1.0",
    "ndarray": "^1.0.19",
    "ndarray-fill": "^1.0.2",
    "ndarray-unpack": "^1.0.0",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "three-mesh-bvh": "^0.7.3",
    "xml2js": "0.6.2"
  },
  "peerDependencies": {
    "three": "^0.x.x"
  }
}
